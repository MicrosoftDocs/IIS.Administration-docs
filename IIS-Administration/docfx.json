{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*swagger.json"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "searchScope": [
        "IIS"
      ],
      "ms.prod": "iis",
      "ms.technology": "iis-administration",
      "ms.topic": "conceptual",
      "author": "shirhatti",
      "ms.author": "soshir"
    },
    "fileMetadata": {
      "ms.topic": {
        "iis-administration/api/**/**.md": "reference",
        "iis-administration/api/**/**.json": "reference"
      },
      "ms.date": {
        "iis-administration/api/**/**.json": "10/25/2017"
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
